{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "cyclomatic-complexity": [
      true,
      10
    ],
    "import-name": false,
    "ter-prefer-arrow-callback": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "ban-keywords"
    ]
  }
}